{
    "compilerOptions": {
      "target": "es2022",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "out",
      "rootDir": ".",
      "noUncheckedIndexedAccess": true,
      "noImplicitReturns": true
    },
    "include": [
      "src/**/*.ts",
      "test/**/*.ts",
      "main.ts"
    ],
    "exclude": [
      "node_modules"
    ]
  }